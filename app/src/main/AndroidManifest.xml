<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.nepalicoders.exercisenotifyproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleTextActivity"
            android:label="@string/title_activity_simple_text" >
        </activity>
        <activity
            android:name=".SimplePictureActivity"
            android:label="@string/title_activity_simple_picture" >
        </activity>
        <activity
            android:name=".SimpleListActivity"
            android:label="@string/title_activity_simple_list" >
        </activity>
    </application>

</manifest>